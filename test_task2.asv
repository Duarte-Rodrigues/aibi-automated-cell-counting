close all; clear all;
% Obter ROI
input = im2double(imread('train-images\train_images\20151115_172901.tiff'));
[ROI, topLine, leftColumn] = getROI(input, 3);
figure, imshow(ROI)
%%
% Passar para cinza e suavização
gray = rgb2gray(ROI);
med = medfilt2(gray);
figure, imshow(med)
%%
% suavizar linhas ligeiramente
SE = strel('disk', 10);
open = imclose(med, SE);
figure, imshow(open)
figure, imhist(open)
%%
th = multithresh(open, 2);
bw = imqua(open);
figure, imshow(bw)
%%
% Obter gradiente
[Gmag, Gdir] = imgradient(open, 'sobel');
figure, imshow(Gmag)
%%
bw = imbinarize(Gmag);
figure, imshow(bw), hold on;
%%
[centers, radii] = imfindcircles (Gmag, [11 50]); 
viscircles(centers, radii)
%%
figure, imshow(imsubtract(med, open))

%%
% binarizar
bw = imbinarize(Gmag);
figure, imshow(bw), hold on;
%% 
SE = strel('square', 10);
closed = imclose(bw, SE);
figure, imshow(closed)
%%
[centers, radii] = imfindcircles (bw, [11 50]); 
viscircles(centers, radii)
